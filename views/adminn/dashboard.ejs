                    <div class="small">Logged in as:</div>
                    
                    
                    <img src="<%=user.imgUrl%> " alt="" class="test2">
                 <%= user.name %> 
                </div>
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    <div class="card mt-5">
                        <div class="card-header current_day">
                            <i class="fas fa-table me-1"></i>
                            <!-- Daily Logs -->Current Voting
                        </div>
                        <div class="card-body">
                            <table class="table table-light table-striped table-hover" id="example" style="width: 100%">
                                <thead>
                                    <tr>
                                        <th scope="col">#</th>
                                       
